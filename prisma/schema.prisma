generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ToDo {
  id    Int     @id @default(autoincrement())
  ToDos ToDos[]
}

model ToDos {
  id     Int    @id @default(autoincrement())
  text   String
  todo   ToDo   @relation(fields: [toDoId], references: [id])
  toDoId Int
}
